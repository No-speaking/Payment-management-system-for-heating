package com.flower.hot.model.dx;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import org.springframework.format.annotation.DateTimeFormat;

import com.fasterxml.jackson.annotation.JsonFormat;

@Entity
@Table(name="wxxx")
public class WxxxModel {
	@Id
	private int wno=0;
	private String wtms=null;
	//@JsonFormat(pattern = "yyyy-MM-dd") 
	//@DateTimeFormat(pattern="yyyy-MM-dd")
	private String tdate=null;
	private int wnumber=0;
	private String waddr=null;
	public int getWno() {
		return wno;
	}
	public void setWno(int wno) {
		this.wno = wno;
	}
	public String getWtms() {
		return wtms;
	}
	public void setWtms(String wtms) {
		this.wtms = wtms;
	}
	
	public String getTdate() {
		return tdate;
	}
	public void setTdate(String tdate) {
		this.tdate = tdate;
	}
	public int getWnumber() {
		return wnumber;
	}
	public void setWnumber(int wnumber) {
		this.wnumber = wnumber;
	}
	public String getWaddr() {
		return waddr;
	}
	public void setWaddr(String waddr) {
		this.waddr = waddr;
	}
	
	

}
