
<div class="row">
            <div class="col-md-12">
             
                <form name="sentMessage" id="headportAddForm" action="../Headport/add.mvc" method="post" novalidate>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>供热点编码:</label>
                            <input type="text" class="form-control" name="portno" id="portno" required data-validation-required-message="Please enter your name.">
                            <p class="help-block"></p>
                        </div>
                    </div>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>供热点名称:</label>
                            <input type="tel" class="form-control" name="portname"  id="portname" required data-validation-required-message="Please enter your phone number.">
                        </div>
                        
                        
                            <div class="control-group form-group">
                        <div class="controls">
                            <label>范围:</label>
                            <input type="text" class="form-control" name="scope"  id="scope" required data-validation-required-message="Please enter your name.">
                            <p class="help-block"></p>
                        </div>
                        
                        
                            <div class="control-group form-group">
                        <div class="controls">
                            <label>地址:</label>
                            <input type="text" class="form-control" name="portaddr"  id="portaddr" required data-validation-required-message="Please enter your name.">
                            <p class="help-block"></p>
                        </div>
                        
                            <div class="control-group form-group">
                        <div class="controls">
                            <label>联系地址:</label>
                            <input type="text" class="form-control" name="porttel"  id="porttel" required data-validation-required-message="Please enter your name.">
                            <p class="help-block"></p>
                        </div>
                        
                        
                        
                        
                    </div>
                    
                    <div id="success"></div>
                    <!-- For success/fail messages -->
                    <button type="submit" class="btn btn-primary">提交</button>
                </form>
            </div>

        </div>