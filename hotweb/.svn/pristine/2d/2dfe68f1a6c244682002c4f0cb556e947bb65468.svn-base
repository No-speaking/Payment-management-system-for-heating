
<div class="row">
            <div class="col-md-12">
                
                <form name="sentMessage" id="typeModifyForm" action="../Type/modify.mvc" method="post" novalidate>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>类型编码:</label>
                            <span type="text" class="form-control" name="tno" id="name" required data-validation-required-message="Please enter your name.">
                            <p class="help-block"></p>
                        </div>
                    </div>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>类型名称:</label>
                            <span type="text" class="form-control" name="tname" id="phone" required data-validation-required-message="Please enter your phone number.">
                        </div>
                    </div>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>等级:</label>
                            <span type="text" class="form-control" name="dengji" id="phone" required data-validation-required-message="Please enter your phone number.">
                        </div>
                    </div>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>明细:</label>
                            <span type="text" class="form-control" name="mingxi" id="phone" required data-validation-required-message="Please enter your phone number.">
                        </div>
                    </div>
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>备注:</label>
                            <span type="text" class="form-control" name="beizhu" id="phone" required data-validation-required-message="Please enter your phone number.">
                        </div>
                    </div>
                     
                    <div id="success"></div>
                    <!-- For success/fail messages -->
                   
                </form>
            </div>

        </div>